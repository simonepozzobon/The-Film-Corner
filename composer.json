{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "barryvdh/laravel-debugbar": "^2.3",
        "buggedcom/phpvideotoolkit": "^2.2",
        "doctrine/dbal": "^2.5",
        "egeloen/google-map": "^2.0",
        "egeloen/serializer": "^1.0",
        "intervention/image": "^2.3",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^2.1",
        "mews/purifier": "^2.0",
        "pragmarx/tracker": "^3.2",
        "predis/predis": "^1.1",
        "rairlie/laravel-locking-session": "^1.0",
        "spatie/laravel-activitylog": "*",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
        "teamtnt/tntsearch": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "lanin/laravel-api-debugger": "^0.3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
